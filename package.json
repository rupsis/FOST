{
  "name": "fost",
  "displayName": "FOST",
  "description": "Simple Format On Save Toggle expose in working UI",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/rupsis/FOST"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "fost.toggleFormatOnSave",
      "title": "Toggle Format On Save",
      "icon": "$(issue-reopened)"
    }],
    "menus": {
      "editor/title": [{
          "command": "fost.toggleFormatOnSave",
          "group": "navigation",
          "when": "editorLangId == javascript"
      }]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
